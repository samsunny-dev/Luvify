index 0000000..b536621
--- /dev/null
+++ b/Backend/routes/chat.js
@@ -0,0 +1,210 @@
+import express from 'express';
+import { userAuthenticate } from '../middleware/auth.js';
+import Message from '../models/Message.js';
+import Conversation from '../models/Conversation.js';
+import User from '../models/User.js';
+
+const router = express.Router();
+
+// Get chat history with a user
+router.get('/:receiverId', userAuthenticate, async (req, res) => {
+  try {
+    const conversation = await Conversation.findOne({
+      participants: { $all: [req.user.id, req.params.receiverId] }
+    });
+
+    if (!conversation) {
+      return res.json({ messages: [] });
+    }
+
+    const messages = await Message.find({ conversation: conversation._id })
+      .populate('sender', 'name')
+      .sort('createdAt');
+
+    res.json({ messages });
+  } catch (error) {
+    res.status(500).json({ error: error.message });
+  }
+});
+
+// Send a message
+router.post('/send', userAuthenticate, async (req, res) => {
+  try {
+    const { receiverId, content, type = 'text' } = req.body;
+
+    // Find or create conversation
+    let conversation = await Conversation.findOne({
+      participants: { $all: [req.user.id, receiverId] }
+    });
+
+    if (!conversation) {
+      conversation = new Conversation({
+        participants: [req.user.id, receiverId],
+        type: 'private'
+      });
