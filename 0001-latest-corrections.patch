+  FaHeart,
+  FaUsers,
+  FaCalendarAlt,
+  FaBell,
+  FaCog,
+  FaQuestionCircle,
+} from 'react-icons/fa';
+
+const MotionBox = motion(Box);
+
+const SidebarItem = ({ icon, label, path, isActive }) => {
+  const navigate = useNavigate();
+  const activeBg = useColorModeValue('pink.100', 'pink.700');
+  const hoverBg = useColorModeValue('pink.50', 'pink.600');
+
+  return (
+    <Tooltip label={label} placement="right" hasArrow>
+      <MotionBox
+        whileHover={{ scale: 1.1 }}
+        whileTap={{ scale: 0.95 }}
