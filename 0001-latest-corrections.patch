+++ b/frontend/src/services/socketService.js
@@ -0,0 +1,59 @@
+import { io } from 'socket.io-client';
+
+class SocketService {
+  constructor() {
+    this.socket = null;
+  }
+
+  connect(token) {
+    this.socket = io(process.env.REACT_APP_WEBSOCKET_URL || 'http://localhost:3001', {
+      auth: { token },
+      transports: ['websocket'],
+    });
+
+    this.socket.on('connect', () => {
+      console.log('Connected to WebSocket server');
+    });
+
+    this.socket.on('disconnect', () => {
+      console.log('Disconnected from WebSocket server');
+    });
+
+    return this.socket;
+  }
+
+  disconnect() {
+    if (this.socket) {
+      this.socket.disconnect();
+      this.socket = null;
+    }
+  }
+
+  // Chat events
+  onNewMessage(callback) {
+    this.socket?.on('new_message', callback);
+  }
+
+  onMessageReaction(callback) {
+    this.socket?.on('message_reaction', callback);
+  }
+
+  onUserTyping(callback) {
+    this.socket?.on('user_typing', callback);
+  }
+
+  // Emit events
+  sendMessage(message) {
+    this.socket?.emit('send_message', message);
+  }
+
+  sendReaction(messageId, reaction) {
+    this.socket?.emit('react_to_message', { messageId, reaction });
+  }
+
+  sendTypingStatus(conversationId, isTyping) {
+    this.socket?.emit('typing_status', { conversationId, isTyping });
+  }
+}
+
+export default new SocketService();
