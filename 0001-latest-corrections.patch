+const { createCanvas } = require('canvas');
+const fs = require('fs');
+const path = require('path');
+
+// Function to create a test image
+function createTestImage(filename, width, height, text) {
+  const canvas = createCanvas(width, height);
+  const ctx = canvas.getContext('2d');
+
+  // Fill background
+  ctx.fillStyle = '#f0f0f0';
+  ctx.fillRect(0, 0, width, height);
+
+  // Add text
+  ctx.fillStyle = '#333333';
+  ctx.font = '20px Arial';
+  ctx.textAlign = 'center';
+  ctx.textBaseline = 'middle';
+  ctx.fillText(text, width / 2, height / 2);
+
+  // Save to file
+  const buffer = canvas.toBuffer('image/jpeg');
+  fs.writeFileSync(path.join(__dirname, 'testData', filename), buffer);
+}
+
+// Create test images
+createTestImage('test-image.jpg', 400, 300, 'Test Image');
+createTestImage('test-image1.jpg', 400, 300, 'Test Image 1');
+createTestImage('test-image2.jpg', 400, 300, 'Test Image 2');
+createTestImage('test-face-image.jpg', 400, 400, 'Face Test Image');
+
+console.log('Test images generated successfully!');
