+        <IconButton
+          icon={<Icon as={icon} w={6} h={6} />}
+          aria-label={label}
+          variant="ghost"
+          color={isActive ? 'pink.500' : 'gray.500'}
+          bg={isActive ? activeBg : 'transparent'}
+          _hover={{ bg: hoverBg }}
+          onClick={() => navigate(path)}
+          size="lg"
+          mb={4}
+        />
+      </MotionBox>
+    </Tooltip>
+  );
+};
+
+const Sidebar = () => {
+  const location = useLocation();
+  const bgColor = useColorModeValue('white', 'gray.800');
+
+  const menuItems = [
+    { icon: FaHome, label: 'Dashboard', path: '/dashboard' },
+    { icon: FaUser, label: 'Profile', path: '/profile' },
+    { icon: FaComments, label: 'Chat', path: '/chat' },
+    { icon: FaHeart, label: 'Matches', path: '/matches' },
+    { icon: FaUsers, label: 'Communities', path: '/communities' },
+    { icon: FaCalendarAlt, label: 'Events', path: '/events' },
+    { icon: FaBell, label: 'Notifications', path: '/notifications' },
+    { icon: FaCog, label: 'Settings', path: '/settings' },
+    { icon: FaQuestionCircle, label: 'Help', path: '/help' },
+  ];
+
+  return (
+    <Box
+      as="nav"
+      pos="fixed"
+      left={0}
+      h="100vh"
+      w="80px"
+      bg={bgColor}
+      boxShadow="0 4px 12px 0 rgba(0, 0, 0, 0.05)"
+      zIndex={100}
+      py={8}
+    >
+      <VStack spacing={4}>
+        {menuItems.map((item) => (
+          <SidebarItem
+            key={item.path}
+            icon={item.icon}
+            label={item.label}
+            path={item.path}
+            isActive={location.pathname === item.path}
+          />
+        ))}
+      </VStack>
+    </Box>
+  );
+};
+
+export default Sidebar;
