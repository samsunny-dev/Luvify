+++ b/frontend/src/components/Chat/MessageList.jsx
@@ -0,0 +1,325 @@
+import React from 'react';
+import {
+  VStack,
+  HStack,
+  Text,
+  Avatar,
+  Box,
+  Image,
+  IconButton,
+  Link,
+  useColorModeValue,
+  Menu,
+  MenuButton,
+  MenuList,
+  MenuItem,
+  Portal,
+  Tooltip,
+  Wrap,
+  WrapItem,
+  Popover,
+  PopoverTrigger,
+  PopoverContent,
+  PopoverBody,
+  PopoverArrow,
+} from '@chakra-ui/react';
+import {
+  FiDownload,
+  FiPlay,
+  FiPause,
+  FiMoreVertical,
+  FiEdit2,
+  FiTrash2,
+  FiMessageSquare,
+  FiSmile,
+} from 'react-icons/fi';
+
+const ReactionButton = ({ reaction, count, users, isSelected, onToggle }) => {
+  return (
+    <Popover trigger="hover" placement="top">
+      <PopoverTrigger>
+        <Box
+          as="button"
+          px={2}
+          py={1}
+          borderRadius="full"
+          fontSize="sm"
+          bg={isSelected ? 'blue.100' : 'gray.100'}
+          color={isSelected ? 'blue.500' : 'gray.600'}
+          _hover={{ bg: isSelected ? 'blue.200' : 'gray.200' }}
+          onClick={onToggle}
+        >
+          {reaction} {count}
+        </Box>
+      </PopoverTrigger>
+      <PopoverContent width="auto">
+        <PopoverArrow />
+        <PopoverBody>
+          <VStack align="start" spacing={1}>
+            {users.map(user => (
+              <Text key={user.id} fontSize="sm">
+                {user.name}
+              </Text>
+            ))}
+          </VStack>
+        </PopoverBody>
+      </PopoverContent>
+    </Popover>
+  );
+};
+
+const AudioMessage = ({ url }) => {
+  const [isPlaying, setIsPlaying] = React.useState(false);
+  const audioRef = React.useRef(null);
+
+  const togglePlay = () => {
+    if (audioRef.current) {
+      if (isPlaying) {
+        audioRef.current.pause();
+      } else {
+        audioRef.current.play();
+      }
+      setIsPlaying(!isPlaying);
+    }
+  };
+
+  return (
+    <HStack spacing={2} align="center">
+      <IconButton
+        icon={isPlaying ? <FiPause /> : <FiPlay />}
+        size="sm"
+        onClick={togglePlay}
+        aria-label={isPlaying ? 'Pause' : 'Play'}
+      />
+      <audio ref={audioRef} src={url} onEnded={() => setIsPlaying(false)} />
+      <Text fontSize="sm">Voice Message</Text>
+    </HStack>
+  );
+};
+
+const FileMessage = ({ url, fileName, fileSize, mimeType }) => {
+  const isImage = mimeType?.startsWith('image/');
+  const isAudio = mimeType?.startsWith('audio/');
+
+  if (isImage) {
+    return (
+      <Image
+        src={url}
+        alt={fileName}
+        maxW="300px"
+        borderRadius="md"
+        loading="lazy"
+      />
+    );
+  }
+
+  if (isAudio) {
+    return <AudioMessage url={url} />;
+  }
+
+  return (
+    <HStack spacing={2} p={2} bg="gray.100" borderRadius="md">
+      <IconButton
+        icon={<FiDownload />}
+        size="sm"
+        as={Link}
+        href={url}
+        download={fileName}
+        aria-label="Download file"
+      />
+      <Box>
+        <Text fontSize="sm" fontWeight="medium">
+          {fileName}
+        </Text>
+        <Text fontSize="xs" color="gray.500">
+          {(fileSize / 1024).toFixed(1)} KB
+        </Text>
+      </Box>
+    </HStack>
+  );
+};
+
+const Message = ({
+  message,
+  isOwn,
+  onEdit,
+  onDelete,
+  onReply,
+  onReaction,
+  currentUser,
+}) => {
+  const messageBg = useColorModeValue(
+    isOwn ? 'blue.500' : 'gray.100',
+    isOwn ? 'blue.500' : 'gray.700'
+  );
+  const messageColor = isOwn ? 'white' : undefined;
+  const reactionEmojis = ['👍', '❤️', '😂', '😮', '😢', '🙏'];
+
+  return (
+    <Box>
+      {message.replyTo && (
+        <Box
+          ml={!isOwn ? 10 : 0}
+          mr={isOwn ? 10 : 0}
+          mb={1}
+          p={2}
+          bg="gray.50"
+          borderRadius="md"
+          fontSize="sm"
+        >
+          <Text color="gray.500" fontSize="xs" mb={1}>
+            Replying to {message.replyTo.sender.name}
+          </Text>
+          <Text noOfLines={1}>{message.replyTo.content}</Text>
+        </Box>
+      )}
+      
+      <HStack
+        alignSelf={isOwn ? 'flex-end' : 'flex-start'}
+        spacing={2}
+        maxW="70%"
+      >
+        {!isOwn && (
+          <Avatar
+            size="sm"
+            name={message.sender.name}
+            src={message.sender.avatar}
+          />
+        )}
+        <Box>
+          <Box
+            bg={message.type !== 'text' ? 'transparent' : messageBg}
+            color={message.type !== 'text' ? 'inherit' : messageColor}
+            px={4}
+            py={2}
+            borderRadius="lg"
+            borderBottomLeftRadius={!isOwn ? 0 : undefined}
+            borderBottomRightRadius={isOwn ? 0 : undefined}
+          >
+            {message.type === 'text' ? (
+              <Text>{message.content}</Text>
+            ) : (
+              <FileMessage
+                url={message.content}
+                fileName={message.fileName}
+                fileSize={message.fileSize}
+                mimeType={message.mimeType}
+              />
+            )}
+            <HStack spacing={1} mt={1} justify="space-between">
+              <Text fontSize="xs" opacity={0.8}>
+                {new Date(message.timestamp).toLocaleTimeString([], {
+                  hour: '2-digit',
+                  minute: '2-digit',
+                })}
+                {message.isEdited && (
+                  <Text as="span" ml={1} fontSize="xs" color="gray.500">
+                    (edited)
+                  </Text>
+                )}
+              </Text>
+              <Menu>
+                <MenuButton
+                  as={IconButton}
+                  icon={<FiMoreVertical />}
+                  variant="ghost"
+                  size="xs"
+                  color={messageColor}
+                  _hover={{ bg: 'transparent' }}
+                />
+                <Portal>
+                  <MenuList>
+                    <MenuItem
+                      icon={<FiSmile />}
+                      onClick={(e) => {
+                        e.stopPropagation();
+                        // Show reaction picker
+                      }}
+                    >
+                      Add Reaction
+                    </MenuItem>
+                    <MenuItem
+                      icon={<FiMessageSquare />}
+                      onClick={() => onReply(message)}
+                    >
+                      Reply
+                    </MenuItem>
+                    {isOwn && (
+                      <>
+                        <MenuItem
+                          icon={<FiEdit2 />}
+                          onClick={() => onEdit(message)}
+                        >
+                          Edit
+                        </MenuItem>
+                        <MenuItem
+                          icon={<FiTrash2 />}
+                          onClick={() => onDelete(message)}
+                          color="red.500"
+                        >
+                          Delete
+                        </MenuItem>
+                      </>
+                    )}
+                  </MenuList>
+                </Portal>
+              </Menu>
+            </HStack>
+          </Box>
+          {message.reactions && message.reactions.size > 0 && (
+            <Wrap spacing={1} mt={1}>
+              {Array.from(message.reactions).map(([reaction, users]) => (
+                <WrapItem key={reaction}>
+                  <ReactionButton
+                    reaction={reaction}
+                    count={users.length}
+                    users={users}
+                    isSelected={users.some(
+                      u => u.user.toString() === currentUser.id
+                    )}
+                    onToggle={() => onReaction(message, reaction)}
+                  />
+                </WrapItem>
+              ))}
+            </Wrap>
+          )}
+        </Box>
+        {isOwn && (
+          <Avatar
+            size="sm"
+            name={message.sender.name}
+            src={message.sender.avatar}
+          />
+        )}
+      </HStack>
+    </Box>
+  );
+};
+
+const MessageList = ({
+  messages,
+  currentUser,
+  onEdit,
+  onDelete,
+  onReply,
+  onReaction,
+}) => {
+  return (
+    <VStack spacing={4} align="stretch" width="100%">
+      {messages.map((message) => (
+        <Message
+          key={message.id}
+          message={message}
+          isOwn={message.sender.id === currentUser.id}
+          onEdit={onEdit}
+          onDelete={onDelete}
+          onReply={onReply}
+          onReaction={onReaction}
+          currentUser={currentUser}
+        />
+      ))}
+    </VStack>
+  );
+};
+
+export default MessageList;
