@@ -0,0 +1,87 @@
+import React, { useState } from 'react';
+import {
+  Box,
+  VStack,
+  HStack,
+  Text,
+  Input,
+  InputGroup,
+  InputLeftElement,
+  Avatar,
+  Badge,
+  Icon,
+  useColorModeValue,
+} from '@chakra-ui/react';
+import { FiSearch } from 'react-icons/fi';
+
+const ChatSidebar = ({ conversations, activeChat, setActiveChat }) => {
+  const [searchQuery, setSearchQuery] = useState('');
+  const borderColor = useColorModeValue('gray.200', 'gray.700');
+  const hoverBg = useColorModeValue('gray.100', 'gray.700');
+  const activeBg = useColorModeValue('blue.50', 'blue.900');
+
+  const filteredConversations = conversations.filter(
+    (conv) =>
+      conv.name.toLowerCase().includes(searchQuery.toLowerCase()) ||
+      conv.lastMessage?.toLowerCase().includes(searchQuery.toLowerCase())
+  );
+
+  return (
+    <Box
+      w="300px"
+      borderRight="1px"
+      borderColor={borderColor}
+      h="full"
+      overflowY="auto"
+    >
+      <VStack spacing={4} p={4}>
+        <InputGroup>
+          <InputLeftElement pointerEvents="none">
+            <Icon as={FiSearch} color="gray.400" />
+          </InputLeftElement>
+          <Input
+            placeholder="Search conversations..."
+            value={searchQuery}
+            onChange={(e) => setSearchQuery(e.target.value)}
+          />
+        </InputGroup>
+
+        <VStack spacing={2} align="stretch" w="full">
+          {filteredConversations.map((conv) => (
+            <Box
+              key={conv.id}
+              p={3}
+              cursor="pointer"
+              borderRadius="md"
+              bg={activeChat?.id === conv.id ? activeBg : 'transparent'}
+              _hover={{ bg: activeChat?.id !== conv.id ? hoverBg : activeBg }}
+              onClick={() => setActiveChat(conv)}
+            >
+              <HStack spacing={3}>
+                <Avatar size="md" name={conv.name} src={conv.avatar} />
+                <Box flex={1}>
+                  <HStack justify="space-between">
+                    <Text fontWeight="bold">{conv.name}</Text>
+                    <Text fontSize="xs" color="gray.500">
+                      {conv.lastMessageTime}
+                    </Text>
+                  </HStack>
+                  <Text fontSize="sm" noOfLines={1} color="gray.500">
+                    {conv.lastMessage}
+                  </Text>
+                </Box>
+                {conv.unreadCount > 0 && (
+                  <Badge colorScheme="blue" borderRadius="full">
+                    {conv.unreadCount}
+                  </Badge>
+                )}
+              </HStack>
+            </Box>
+          ))}
+        </VStack>
+      </VStack>
+    </Box>
+  );
+};
+
+export default ChatSidebar;
