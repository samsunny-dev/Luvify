+import React, { useEffect } from 'react';
+import { motion, useAnimation, useInView } from 'framer-motion';
+
+const ScrollAnimation = ({ children, direction = 'up', delay = 0, duration = 0.5 }) => {
+  const controls = useAnimation();
+  const ref = React.useRef(null);
+  const isInView = useInView(ref, { once: true, amount: 0.3 });
+
+  const directions = {
+    up: { y: 50 },
+    down: { y: -50 },
+    left: { x: 50 },
+    right: { x: -50 },
+  };
+
+  useEffect(() => {
+    if (isInView) {
+      controls.start({
+        opacity: 1,
+        x: 0,
+        y: 0,
+        transition: {
+          type: 'spring',
+          duration,
+          delay,
+          bounce: 0.3,
+        },
+      });
+    }
+  }, [isInView, controls, delay, duration]);
+
+  return (
+    <motion.div
+      ref={ref}
+      initial={{
+        opacity: 0,
+        ...directions[direction],
+      }}
+      animate={controls}
+    >
+      {children}
+    </motion.div>
+  );
+};
+
+const ScrollFade = ({ children, delay = 0, duration = 0.5 }) => {
+  const controls = useAnimation();
+  const ref = React.useRef(null);
+  const isInView = useInView(ref, { once: true, amount: 0.3 });
+
+  useEffect(() => {
+    if (isInView) {
+      controls.start({
+        opacity: 1,
+        scale: 1,
+        transition: {
+          type: 'spring',
+          duration,
+          delay,
+          bounce: 0.2,
+        },
+      });
+    }
+  }, [isInView, controls, delay, duration]);
+
+  return (
+    <motion.div
+      ref={ref}
+      initial={{
+        opacity: 0,
+        scale: 0.9,
+      }}
+      animate={controls}
+    >
+      {children}
+    </motion.div>
+  );
+};
+
+const ScrollStagger = ({ children, delay = 0, staggerDelay = 0.1 }) => {
+  const controls = useAnimation();
+  const ref = React.useRef(null);
+  const isInView = useInView(ref, { once: true, amount: 0.3 });
+
+  useEffect(() => {
+    if (isInView) {
+      controls.start((i) => ({
+        opacity: 1,
+        y: 0,
+        transition: {
+          type: 'spring',
+          delay: i * staggerDelay + delay,
+          bounce: 0.3,
+        },
+      }));
+    }
+  }, [isInView, controls, delay, staggerDelay]);
+
+  return (
+    <motion.div ref={ref}>
+      {React.Children.map(children, (child, i) => (
+        <motion.div
+          custom={i}
+          initial={{ opacity: 0, y: 20 }}
+          animate={controls}
+        >
+          {child}
+        </motion.div>
+      ))}
+    </motion.div>
+  );
+};
+
+export { ScrollAnimation, ScrollFade, ScrollStagger };
